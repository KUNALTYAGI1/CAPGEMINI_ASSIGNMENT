<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <!-- bean definitions here -->
    
    <bean id="customer" class="springcore_assignment1.Customer">
    <property name="address" ref="address1"/>
    <property name="customerId" value="1" />
    </bean>
    
    <bean id="address1" class="springcore_assignment1.Address">
    <property name="street" value="A" />
    <property name="city" value="B" />
    <property name="state" value="C" />
    <property name="zip" value="D" />
    <property name="country" value="E" />
    </bean>

</beans>